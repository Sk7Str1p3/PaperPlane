<?xml version="1.0" encoding="utf-8"?>
<schemalist>
  <schema path="/app/drey/paper-plane/" id="@app-id@" gettext-domain="@gettext-package@">
    <key name="window-width" type="i">
      <default>900</default>
      <summary>Default window width</summary>
      <description>Default window width</description>
    </key>
    <key name="window-height" type="i">
      <default>600</default>
      <summary>Default window height</summary>
      <description>Default window height</description>
    </key>
    <key name="is-maximized" type="b">
      <default>false</default>
      <summary>Default window maximized behaviour</summary>
      <description>Default window maximized behaviour</description>
    </key>
    <key name="last-used-session" type="s">
      <default>''</default>
      <summary>Last used session</summary>
      <description>The directory name of the last used session</description>
    </key>
    <key name="color-scheme" type="s">
      <choices>
        <choice value="default"/>
        <choice value="light"/>
        <choice value="dark"/>
      </choices>
      <default>'default'</default>
      <summary>Color Scheme</summary>
      <description>The color scheme to be used in the app</description>
    </key>
    <key name="collapsed-archive-row" type="b">
      <default>false</default>
      <summary>Collapsed archive row</summary>
      <description>Whether the archive row is collaped</description>
    </key>
    <key name="archive-row-in-main-menu" type="b">
      <default>false</default>
      <summary>Whether the archive row shown in the main menu</summary>
      <description>Whether the archive row shown in the main menu or in the chat list</description>
    </key>
  </schema>
</schemalist>
